{
  "exclude": "node_modules/**",
  "plugins": [
    "@babel/plugin-proposal-nullish-coalescing-operator",
    [
      "babel-plugin-styled-components",
      {
        "pure": true
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react"
  ]
}
