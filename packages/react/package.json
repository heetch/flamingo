    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-url": "^2.2.2",
    "svgo": "^1.3.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "react-popper": "^1.3.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
