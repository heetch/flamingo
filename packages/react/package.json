    "normalize.css": "^8.0.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.1.1",
    "react-table": "next",
    "rollup": "^1.21.4",
